		<parameter name="strBrowser" value="saucechrome" />
		<parameter name="strLanguage" value="en" />
		<classes>
			<class name="ca.fido.test.tests.connectedhome.desktop.FidoCH_Regression_TC_003_HSIPlanDowngradeTest"></class>
