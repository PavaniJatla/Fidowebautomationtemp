<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Rogers Complete RT Suite - BFA" parallel="none" thread-count="10">
	<parameter name="strBrowser" value="saucechrome" />
	<parameter name="strLanguage" value="en" />
	
	<listeners>
		<listener
			class-name="com.rogers.test.listeners.TestListener" />
		<listener
			class-name="com.rogers.test.listeners.AnnotationTransformer" />
	</listeners>
	
	<test name="TC07 - Rogers - Regression - Perform Rogers AAL - e2e" enabled="true">
	<classes>
			<class
				name="com.rogers.test.tests.buyflows.RogersBFA_TC07_AAL_Test">
			</class>
		</classes>
	</test>

	<test name="TC08 - Rogers - Regression - Perform Rogers HUP - e2e" enabled="false">
	<classes>
			<class
				name="com.rogers.test.tests.buyflows.RogersBFA_TC08_HUP_Test">
			</class>
		</classes>
	</test>
			
	<test name="TC09 - Rogers - Regression - Perform Rogers PPC - e2e" enabled="false">
	<classes>
			<class
				name="com.rogers.test.tests.buyflows.RogersBFA_TC09_PPC_Test">
			</class>
		</classes>
	</test>
		
</suite>