<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Fido Sanity BFA" parallel="none" thread-count="10">
	<parameter name="strBrowser" value="saucechrome"/>
	<parameter name="strLanguage" value="en"/>
	<listeners>
		<listener
			class-name="ca.fido.test.listeners.TestListener" />
		<listener
			class-name="ca.fido.test.listeners.AnnotationTransformer" />
	</listeners>
	
	<test name="TC01 - Fido -Regression - Validate NAC order submission for a NO TERM device using Talk and Text plan -AWS - e2e" enabled="false">
	<classes>
			<class
				name="ca.fido.test.tests.buyflows.Fido_BFA_TC01_NAC_Test">
			</class>
		</classes>
	</test>
	
	<test name="TC04 - Fido - Regression - Perform Fido HUP - e2e" enabled="true">
	<classes>
			<class
				name="ca.fido.test.tests.buyflows.Fido_BFA_TC04_HUP_Test">
			</class>
		</classes>
	</test>

	<test name="TC05 - Fido - Regression - Perform Fido AAL TERM - AWS - e2e" enabled="true">
	<classes>
			<class
				name="ca.fido.test.tests.buyflows.Fido_BFA_TC05_AAL_Term_Test">
			</class>
		</classes>
	</test> 
	
	<test name="TC07 - Fido - Regression - Perform Fido PPC - e2e" enabled="true">
	<classes>
			<class
				name="ca.fido.test.tests.buyflows.Fido_BFA_TC07_PPC_Test">
			</class>
		</classes>
	</test>
	
	
	
</suite>