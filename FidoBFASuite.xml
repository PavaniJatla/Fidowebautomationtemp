<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Fido All Tests BFA" parallel="none" thread-count="10">
	<parameter name="strBrowser" value="chrome"/>
	<parameter name="strLanguage" value="en"/>
	<parameter name="strGroupName" value="buyflow"/>
	<listeners>
		<listener
			class-name="ca.fido.test.listeners.TestListener" />
		<listener
			class-name="ca.fido.test.listeners.AnnotationTransformer" />
	</listeners>
	
		<test name="TC01-Fido-Validate NAC Flow (Single Line) for $0 device- Without payment - EnglishChrome" enabled="true">
	<classes>
			<class
				name="ca.fido.test.tests.buyflows.Fido_BFA_TC01_NAC_SL_ZeroUpfront_NoPayment_Test">
			</class>
		</classes>
	</test>
	
	<test name="TC02 - Fido - Regression - Perform Fido AAL TERM - AWS - e2e" enabled="true">
	<classes>
			<class
				name="ca.fido.test.tests.buyflows.Fido_BFA_TC08_AAL_Term_Test">
			</class>
		</classes>
	</test> 
	
	<test name="TC03 - Fido - Regression - Perform Fido AAL BYOD -AWS - e2e" enabled="true">
	<classes>
			<class
				name="ca.fido.test.tests.buyflows.Fido_BFA_TC07_AAL_BYOD_ExistingFinanceAccount_Test">
			</class>
		</classes>
	</test>
	
	<test name="TC04 - Fido - Regression - Perform Fido HUP - e2e" enabled="true">
	<classes>
			<class
				name="ca.fido.test.tests.buyflows.Fido_BFA_TC03_HUP_withPPC_ExistingSubsidyAccount_Test">
			</class>
		</classes>
	</test>
	
	<test name="TC05 - Fido - Regression - Perform Fido PPC - e2e" enabled="true">
	<classes>
			<class
				name="ca.fido.test.tests.buyflows.Fido_BFA_TC06_PPC_ExistingFinanceAccount_Test">
			</class>
		</classes>
	</test>

</suite>